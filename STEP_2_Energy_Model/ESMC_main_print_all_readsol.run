			#Oil --> Non-Energy
			if sum{c in COUNTRIES, t in PERIODS, h in HOUR_OF_PERIOD[t], td in TYPICAL_DAY_OF_PERIOD[t]}(F_t [c,"NON_ENERGY_OIL", h, td]  ) > 10 then
