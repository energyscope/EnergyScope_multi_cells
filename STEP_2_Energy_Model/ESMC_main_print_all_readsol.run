			#Elec --> SNG	
			if sum{c in COUNTRIES, t in PERIODS, h in HOUR_OF_PERIOD[t], td in TYPICAL_DAY_OF_PERIOD[t]}(F_t [c,"SYN_METHANOLATION", h, td]+F_t [c,"SYN_METHANATION", h, td]  ) > 10 then
